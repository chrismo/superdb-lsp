{
  "scopeName": "source.spq",
  "fileTypes": ["spq"],
  "foldingStartMarker": "\\(\\s*$",
  "foldingStopMarker": "^\\s*\\)",
  "patterns": [
    {
      "name": "keyword.control.spq",
      "match": "\\b(over)\\b"
    },
    {
      "name": "string.quoted.double.spq",
      "begin": "\"",
      "end": "\"",
      "patterns": [
        {
          "name": "constant.character.escape.spq",
          "match": "\\."
        }
      ]
    },
    {
      "name": "comment.line.double-slash.spq",
      "begin": "//",
      "end": "$"
    }
  ]
}
